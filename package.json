    "postcss": "^8.4.32",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "3.2.11",
    "vue-tsc": "^2.0.6"
  },
  "engines": {
    "node": ">= 18.14.0",
