    "cheerio": "1.0.0-rc.12",
    "crypto-js": "^4.2.0",
    "dayjs": "1.10.5",
    "echarts": "^5.4.3",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "jquery": "^3.7.1",
    "jszip": "^3.10.1",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vue": "^3.3.4",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.31",
    "@types/chrome": "^0.0.246",
    "@types/exceljs": "^1.3.2",
